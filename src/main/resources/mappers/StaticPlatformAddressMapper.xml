<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.witshare.mars.dao.mysql.StaticPlatformAddressMapper">

    <insert id="saveOrUpdate" >
        REPLACE INTO
        platform_address (platform_address, create_time, update_time )
        VALUES
        <foreach collection="collection" item="address" separator=",">
            (#{address,jdbcType=VARCHAR}, now(),now())
        </foreach>
    </insert>

</mapper>